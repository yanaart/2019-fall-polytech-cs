def setup():
    
    size(500, 500)
    smooth()
    noLoop()

def draw():

    background(100);

    stroke(136, 29, 203)
    strokeWeight(110)
    line(100, 150, 400, 150)

    stroke(203, 29, 163)
    strokeWeight(60)
    line(100, 250, 400, 250)

    stroke(203, 29, 29)
    strokeWeight(110)
    line(100, 350, 400, 350)
