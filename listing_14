def setup ():
    size (500 , 500)
    smooth ()
    background (255)
    strokeWeight (30)
    noLoop ()

def draw ():
    stroke (20)
    line (50, 200, 150, 300)
    line (100, 200, 200, 300) 
    line (150, 200, 250, 300)
    line (200, 200, 300, 300)
    line (250, 200, 350, 300) 
    line (300, 200, 400, 300)
    line (350, 200, 450, 300)
